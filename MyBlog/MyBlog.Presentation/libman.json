{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "font-awesome@5.13.0",
      "destination": "wwwroot/lib/font-awesome",
      "files": [ "css/all.css", "js/all.js", "js/fontawesome.js", "webfonts/fa-solid-900.woff2", "webfonts/fa-solid-900.woff", "webfonts/fa-solid-900.ttf" ]
    },
    {
      "library": "twitter-bootstrap@4.4.0",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [ "css/bootstrap.min.css", "css/bootstrap.min.css.map", "js/bootstrap.min.js", "js/bootstrap.bundle.min.js", "js/bootstrap.bundle.min.js.map" ]
    },
    {
      "library": "jquery@3.5.0",
      "destination": "wwwroot/lib/jquery/",
      "files": [ "jquery.min.js" ]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/",
      "files": [ "jquery.validate.unobtrusive.min.js" ]
    }
  ]
}